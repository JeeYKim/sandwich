<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN"
       "http://tiles.apache.org/dtds/tiles-config_2_1.dtd">

<tiles-definitions>

	<definition name="layout" template="/WEB-INF/tiles/layout/layout.jsp">
		<put-attribute name="header" value="/WEB-INF/tiles/tile/header.jsp" />		
		<put-attribute name="body" value="/WEB-INF/tiles/tile/body.jsp" />
		<put-attribute name="footer" value="/WEB-INF/tiles/tile/footer.jsp" />
	</definition>

	 <definition name="main" extends="layout">
        <put-attribute name="body" expression="/WEB-INF/view/main.jsp" />
 	</definition>

 	<definition name="memberNoticeList" extends="layout">
        <put-attribute name="body" expression="/WEB-INF/view/notice/memberNoticeList.jsp" />
 	</definition>
 	
 	<definition name="memberNoticeView" extends="layout">
        <put-attribute name="body" expression="/WEB-INF/view/notice/memberNoticeView.jsp" />
 	</definition>
 	
 	
</tiles-definitions>
